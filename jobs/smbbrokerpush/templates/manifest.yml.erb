---
applications:
- name: "<%= p('app_name') %>"
  buildpacks:
  - binary_buildpack
  routes:
  - route: "<%= p('app_name') + '.' + p('app_domain') %>"
  memory: "<%= p('memory_in_mb') %>M"
  env:
    USERNAME: "<%= p('username') %>"
    PASSWORD: "<%= p('password') %>"
    UAA_CLIENT_ID: "<%= p('credhub.uaa_client_id') %>"
    UAA_CLIENT_SECRET: "<%= p('credhub.uaa_client_secret') %>"
